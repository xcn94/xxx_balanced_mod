		<Row ResourceType="RESOURCE_TURTLES" YieldType="YIELD_PRODUCTION" YieldChange="1"/>
		<Row ResourceType="RESOURCE_TOBACCO" YieldType="YIELD_FOOD" YieldChange="1"/>
	</Resource_YieldChanges>

	<Feature_YieldChanges>
		<Update>
			<Where FeatureType="FEATURE_MARSH" YieldType="YIELD_FOOD"/>
			<Set YieldChange="2"/>
		</Update>
		<Update>
			<Where FeatureType="FEATURE_OASIS" YieldType="YIELD_GOLD"/>
