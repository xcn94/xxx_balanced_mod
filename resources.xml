			<Where ResourceType="RESOURCE_CRABS" YieldType="YIELD_GOLD"/>
			<Set YieldChange="3"/>
		</Update>
		<Update>
			<Where ResourceType="RESOURCE_COPPER" YieldType="YIELD_GOLD"/>
			<Set YieldChange="3"/>
		</Update>
		<Update>
			<Where ResourceType="RESOURCE_FISH" YieldType="YIELD_FOOD"/>
			<Set YieldChange="2"/>
		</Update>
		<Row ResourceType="RESOURCE_STONE" YieldType="YIELD_GOLD" YieldChange="1"/>
		<Row ResourceType="RESOURCE_COFFEE" YieldType="YIELD_FOOD" YieldChange="1"/>
		<Update>
			<Where ResourceType="RESOURCE_COTTON" YieldType="YIELD_GOLD"/>
			<Set YieldChange="3"/>
		</Update>
		<Row ResourceType="RESOURCE_COTTON" YieldType="YIELD_PRODUCTION" YieldChange="1"/>
		<Row ResourceType="RESOURCE_INCENSE" YieldType="YIELD_PRODUCTION" YieldChange="1"/>
		<Row ResourceType="RESOURCE_JADE" YieldType="YIELD_PRODUCTION" YieldChange="1"/>
