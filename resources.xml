	</Feature_YieldChanges>
	
	<Terrain_YieldChanges>
		<Update>
			<Where TerrainType="TERRAIN_SNOW_HILLS" YieldType="YIELD_PRODUCTION"/>
			<Set YieldChange="2"/>
		</Update>
		<Update>
			<Where TerrainType="TERRAIN_DESERT_HILLS" YieldType="YIELD_PRODUCTION"/>
			<Set YieldChange="2"/>
		</Update>
	</Terrain_YieldChanges>
	
	<Resource_ValidFeatures>
	</Resource_ValidFeatures>
</GameData>

